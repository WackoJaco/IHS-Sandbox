<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__Closed__c</fullName>
    <deprecated>false</deprecated>
    <description>Determines when closed based on the risk's Status. Defaults to true when the Status is Closed. Set to false for Open.</description>
    <externalId>false</externalId>
    <formula>If( ISPICKVAL( pse__Status__c, 'Open'), false, true)</formula>
    <label>Closed</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
